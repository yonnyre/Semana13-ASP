@model IEnumerable<Ejercicio3_Semana13.ViewModels.vwPedidoAño>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaPedido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaEntrega)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Cargo)
        </th>
