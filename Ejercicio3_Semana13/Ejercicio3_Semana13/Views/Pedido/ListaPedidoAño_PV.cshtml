            <td>
                @Html.DisplayFor(modelItem => item.FechaEntrega)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Cargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vendedor)
            </td>


        </tr>
    }
</table>
